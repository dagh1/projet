<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="apple-touch-icon" sizes="76x76">
    <link rel="icon" type="image/png" href="../assets/img/favicon.png"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <title>Turbo - Bootstrap Material Admin Dashboard Template</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport'/>
    <meta name="viewport" content="width=device-width"/>
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        <!-- Bootstrap core CSS     -->
        <link href="{{ asset('administration/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <!--  Material Dashboard CSS    -->
        <link href="{{ asset('administration/css/turbo.css') }}" rel="stylesheet"/>
        <!--  CSS for Demo Purpose, don't include it in your project     -->
        <link href="{{ asset('administration/css/demo.css') }}" rel="stylesheet"/>

        <link href="{{ asset('administration/css/modification.css') }}" rel="stylesheet"/>

        <link rel="stylesheet" type="text/css"
              href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons"/>
        <link href="{{ asset('administration/vendors/material-design-iconic-font/dist/css/material-design-iconic-font.min.css') }}"
              rel="stylesheet">
    {% endblock %}
</head>
<body>
<div class="wrapper">
    {{ include('administration/includes/sidebar.html.twig') }}
    <div class="main-panel">
        {{ include('administration/includes/nav.html.twig') }}
        <div class="content">
            <div class="container-fluid">
                {% block body %}
                {% endblock %}
            </div>
        </div>
        {{ include('administration/includes/footer.html.twig') }}
    </div>
</div>
{% block javascripts %}

    <!--   Core JS Files   -->
    <script src="{{ asset('administration/vendors/jquery-3.1.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('administration/vendors/jquery-ui-1.12.0/jquery-ui.js') }}"></script>
    <script src="{{ asset('administration/vendors/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('administration/vendors/material.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('administration/vendors/perfect-scrollbar.jquery.min.js') }}" type="text/javascript"></script>
    <!-- Forms Validations Plugin -->
    <script src="{{ asset('administration/vendors/jquery.validate.min.js') }}"></script>
    <!--  Plugin for Date Time Picker and Full Calendar Plugin-->
    <script src="{{ asset('administration/vendors/moment.min.js') }}"></script>
    <!--  Charts Plugin -->
    <script src="{{ asset('administration/vendors/chartist.min.js') }}"></script>
    <!--  Plugin for the Wizard -->
    <script src="{{ asset('administration/vendors/jquery.bootstrap-wizard.js') }}"></script>
    <!--  Notifications Plugin    -->
    <script src="{{ asset('administration/vendors/bootstrap-notify.js') }}"></script>
    <!-- DateTimePicker Plugin -->
    <script src="{{ asset('administration/vendors/bootstrap-datetimepicker.js') }}"></script>
    <!-- Vector Map plugin -->
    <script src="{{ asset('administration/vendors/jquery-jvectormap.js') }}"></script>
    <!-- Sliders Plugin -->
    <script src="{{ asset('administration/vendors/nouislider.min.js') }}"></script>
    <!--  Google Maps Plugin    -->
    <!-- Select Plugin -->
    <script src="{{ asset('administration/vendors/jquery.select-bootstrap.js') }}"></script>
    <!--  DataTables.net Plugin    -->
    <script src="{{ asset('administration/vendors/jquery.datatables.js') }}"></script>
    <!-- Sweet Alert 2 plugin -->
    <script src="{{ asset('administration/vendors/sweetalert2.js') }}"></script>
    <!--	Plugin for Fileupload, full documentation here: http://www.jasny.net/bootstrap/javascript/#fileinput -->
    <script src="{{ asset('administration/vendors/jasny-bootstrap.min.js') }}"></script>
    <!--  Full Calendar Plugin    -->
    <script src="{{ asset('administration/vendors/fullcalendar.min.js') }}"></script>
    <!-- TagsInput Plugin -->
    <script src="{{ asset('administration/vendors/jquery.tagsinput.js') }}"></script>
    <!-- Material Dashboard javascript methods -->
    <script src="{{ asset('administration/js/turbo.js') }}"></script>
    <!-- Material Dashboard DEMO methods, don't include it in your project! -->
    <script src="{{ asset('administration/js/demo.js') }}"></script>

{% endblock %}
</body>
</html>
